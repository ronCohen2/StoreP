{
  "name": "express-ts-template",
  "version": "1.0.0",
  "description": "Simple starter template for ExpressJs and TypeScript based on expressjs/generator and TypeScript-Node-Starter",
  "main": "server.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint && npm run serve",
    "serve": "node dist/server.js",
    "watch": "nodemon --exec ts-node -- ./src/server.ts",
    "watch-node": "nodemon dist/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "client": "cd client && npm start",
    "server": " npm run watch",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.5",
    "@types/jquery": "^3.3.31",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongoose": "^5.5.11",
    "@types/react-sidebar": "^3.0.0",
    "@types/reactstrap": "^8.0.1",
    "@types/validator": "^10.11.2",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "class-validator": "^0.9.1",
    "cors": "^2.8.5",
    "email-validator": "^2.0.4",
    "express": "^4.16.2",
    "http-errors": "^1.7.1",
    "i": "^0.3.6",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "mdbreact": "^4.19.1",
    "mongoose": "^5.6.8",
    "morgan": "^1.9.1",
    "npm": "^6.10.2",
    "pug": "^2.0.0-rc.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-sidebar": "^3.0.2",
    "reactstrap": "^8.0.1",
    "tsc-watch": "^2.2.1",
    "twilio": "^2.1.1",
    "typescript-bootstrap": "^1.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/http-errors": "^1.6.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.0",
    "concurrently": "^3.6.1",
    "nodemon": "^1.19.1",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "validator": "^11.1.0"
  }
}
